#include<bits/stdc++.h>
using namespace std;
int isvowel(char ch)
{
if(ch=='a'||ch=='e'||ch=='i'||ch=='o'||ch=='u')
return 1;
else
return 0;
}
int main()
{
    int t; cin>>t;
    while(t--)
    {
        string s; cin>>s;
        int flag=0;
        for(int i=0; i<s.length() ; i++)
        {
            if(isvowel(s[i])||s[i]=='n')
                     continue;
            else
            {
                if(!isvowel(s[i+1]))
                {
                    flag=1;
                    break;
                }
            }
        }
        
        if(flag==1) cout<<"NO"<<endl;
        
        else cout<<"YES"<<endl;
    }
    return 0;
}
